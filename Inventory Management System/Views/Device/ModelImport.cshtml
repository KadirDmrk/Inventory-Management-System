@model Inventory_Management_System.Models.Class.Hardware.Model
@{
    ViewBag.Title = "ModelImport";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using (Html.BeginForm("ModelUpdate", "Device", FormMethod.Post))
{
    <div class="form-group">

        @Html.LabelFor(x => x.ModelID)
        @Html.TextBoxFor(x => x.ModelID, new { @class = "form-control" })

        @Html.LabelFor(x => x.ModelName)
        @Html.TextBoxFor(x => x.ModelName, new { @class = "form-control" })

    </div>
    <button class="btn btn-warning"> Model Güncelle </button>
}