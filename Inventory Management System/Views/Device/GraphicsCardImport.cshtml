@model Inventory_Management_System.Models.Class.Hardware.GraphicsCard
@{
    ViewBag.Title = "GraphicsCardImport";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using (Html.BeginForm("GraphicsCardUpdate", "Device", FormMethod.Post))
{
    <div class="form-group">

        @Html.LabelFor(x => x.GraphicsCardID)
        @Html.TextBoxFor(x => x.GraphicsCardID, new { @class = "form-control" })

        @Html.LabelFor(x => x.GraphicsCardName)
        @Html.TextBoxFor(x => x.GraphicsCardName, new { @class = "form-control" })

    </div>
    <button class="btn btn-warning"> Ekran Kartı Güncelle </button>
}




