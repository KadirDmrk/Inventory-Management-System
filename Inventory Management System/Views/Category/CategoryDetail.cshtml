@using Inventory_Management_System.Models.Class
@model List<Product>

@{
    ViewBag.Title = "CategoryDetail";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />


<ol class="breadcrumb" mb-4>
    <li class="breadcrumb-item active">@ViewBag.d </li>
</ol>

<br />




<table class="table table-hover" id="tbl1">
    <thead>


        <tr>
            <th>ÜRÜN ADI</th>
            <th>MARKASI </th>
            <th>SERİ NUMARASI </th>
            <th>DURUM</th>
        </tr>
    </thead>
    @foreach (var x in Model)
    {
        <tr>
            <td> @x.ProductName</td>
            <td> @x.ProductBrand</td>
            <td> @x.SerialNumber</td>
            <td> @x.IsActive</td>
        </tr>
    }
</table>
<button onclick="window.history.back()" class="back-button">Geriye Dön</button>

<link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.2/css/bootstrap.css" rel="stylesheet" />
<link href="https://cdn.datatables.net/1.10.22/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.10.22/js/dataTables.bootstrap4.min.js"></script>

<script>
    $('#tbl1').dataTable({
        "language": {
            "url": "//cdn.datatables.net/plug-ins/1.10.12/i18n/Turkish.json"
        }
    });
</script>
