@using Inventory_Management_System.Models.Class
@model List<Employees>

@{
    ViewBag.Title = "DepartmentDetail";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />


<ol class="breadcrumb" mb-4>
    <li class="breadcrumb-item active">Departman: @ViewBag.d </li>
</ol>

<br />
<table class="table table-hover">

    <tr>
        <th>AD</th>
        <th>SOYAD</th>
        <th>ZİMMETLİ</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.EmployeeName</td>
            <td>@x.EmployeeSurname</td>
            <td><a href="~/Department/DepartmentEmployeesDebit/@x.EmployeeID" class="btn btn-default" style="background-color:cadetblue">Ürünler</a></td>
        </tr>
    }
</table>

<button onclick="window.history.back()" class="back-button">Geriye Dön</button>
